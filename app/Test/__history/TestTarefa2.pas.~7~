unit TestTarefa2;
{

  Delphi DUnit Test Case
  ----------------------
  This unit contains a skeleton test case class generated by the Test Case Wizard.
  Modify the generated code to correctly setup and call the methods from the unit 
  being tested.

}

interface

uses
  TestFramework, Vcl.ComCtrls, controller.interfaces, model.thread,
  controller.thread, system.SysUtils;

type
  // Test methods for class TControllerThread

  TestTControllerThread = class(TTestCase)
  strict private
    FPgBar: TProgressBar;

  public
    procedure SetUp; override;
    procedure TearDown; override;
  published
    procedure Testexecutar;
  end;

implementation

procedure TestTControllerThread.SetUp;
begin
  FPgBar := TProgressBar.Create(nil);
end;

procedure TestTControllerThread.TearDown;
begin
  if FPgBar <> nil then
    freeandnil(FPgBar);
end;

procedure TestTControllerThread.Testexecutar;
var
  ReturnValue: iControllerThread;
  aTempo: Integer;
begin

  ReturnValue := FControllerThread.executar(100, FPgBar);
  // TODO: Validate method results
end;

initialization
  // Register any test cases with the test runner
  RegisterTest(TestTControllerThread.Suite);
end.

